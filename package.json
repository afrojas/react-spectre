{
  "name": "react-spectre",
  "version": "0.0.1-alpha.5",
  "description": "React components for Spectre.css",
  "repository": {
    "type": "git",
    "url": "git://github.com/afrojas/react-spectre.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "babel-node tools/build-cli.js",
    "watch-dev": "concurrently --kill-others 'npm run watch-lib' 'npm run watch-dist'",
    "watch-lib": "babel src --watch --out-dir lib",
    "watch-dist": "webpack --watch",
    "watch-docs": "concurrently 'webpack-dev-server --config webpack/docs.config.babel.js --content-base dist --port 9000' 'npm run watch-dist' 'babel-node tools/docstest.js'",
    "test": "mocha test/.setup.js test/**/*-test.js",
    "patch": "release patch",
    "minor": "release minor",
    "major": "release major",
    "release": "release"
  },
  "author": "Andr√©s Rojas <afrojas@gmail.com>",
  "bugs": {
    "url": "https://github.com/afrojas/react-spectre/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "6.9.0",
    "chai": "^3.5.0",
    "child-process-promise": "^2.0.2",
    "chokidar": "1.5.2",
    "colors": "^1.1.2",
    "concurrently": "2.1.0",
    "copy-webpack-plugin": "3.0.1",
    "enzyme": "^2.3.0",
