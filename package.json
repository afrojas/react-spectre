    "jsdom": "^9.2.1",
    "json-loader": "0.5.4",
    "lodash-compat": "^3.10.2",
    "mocha": "^2.5.3",
    "output-file-sync": "^1.1.1",
    "raw-loader": "0.5.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^15.1.0",
    "release-script": "^1.0.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "yargs": "4.7.1"
  },
